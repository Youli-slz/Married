.Card{
     height:100%; 
     /* border-top: 2rpx solid #f0eff5;  */
    background-color: #fff;
}

.css_line{
    border-top: 2rpx solid #f0eff5; 
}
.Card_empty{
    text-align: center;
    margin-top: 250rpx;
}
.Card_empty image{
    width: 374rpx;
    height: 381rpx;
}
.empty_text{
    margin-top: 24rpx;
    color: #b2b2b2;
    font-size: 32rpx;
}

.create_btn{
    position: fixed;
    bottom: 48rpx;
    left: 200rpx;
    width: 350rpx;
    height: 80rpx;
    background-color: #ff1c63;
    border-radius: 50rpx;
    color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 36rpx;
}

.create_btn :active{
     opacity: 0.7; 
     background-color: #f0eff5;
}
