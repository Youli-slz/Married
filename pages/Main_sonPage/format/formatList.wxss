
.footBar{
    width:100%;
    background-color: #fff;
    position: fixed;
    bottom:0;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.line{
    width: 1rpx;
    height: 50rpx;
}

.icon_tab{
    display: flex;
    flex-direction: column;
    align-items: center;
}

.icon_tab image{
    width:49rpx;
    height: 49rpx;
}

.icon_tab_center{
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-bottom: 43rpx;
}

.icon_tab_center image{
    width: 92rpx;
    height: 92rpx;
}
.icon_words{
    font-size: 22rpx;
    color: #ff1c63
}

.format{
    display: flex;
    justify-content: space-around;
    align-items: center;
}

swiper{
    margin-top: 34rpx;
    height: 1023rpx;
    width: 638rpx;
}

swiper-item .backImg, .backImg image{
    width: 100%;
    height: 100%;
}
.left,.right{
    padding: 0 10rpx;
}
.left image, .right image{
    width: 27rpx;
    height: 49rpx;
}

.imgIcon{
    position: relative;
    bottom: 400rpx;
    left: 300rpx;
}

.imgIcon image{
    width: 70rpx;
    height: 70rpx;
}
