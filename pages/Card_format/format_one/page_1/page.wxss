.card_page{
    width: 100%;
    height: 100%;
}
.card_back{
    width: 100%;
    height: 100%;
    background-image: url("http://7xld1x.com1.z0.glb.clouddn.com/FiMXnfTW8pYcz5TQbxPDuvByPYWQ");
    background-repeat:  no-repeat;
    background-size: 100% 100%;
    animation: one 1s;
    display: flex;
    justify-content: center;
    align-items: center;
}

.card_back image{
    width: 592rpx;
    height: 878rpx;
    animation: two 1s;
    animation-delay:1s;
    animation-fill-mode: backwards;
}
@keyframes two {
    0%{
        transform: scale(0.3,0.3)
    }
    100%{
        transform: scale(1,1)
    }
}

@keyframes one{
    0%{
        opacity: 0;
    }
    100%{
        opacity: 1;
    }
}

.img image{
    width: 588rpx;
    height: 874rpx;
    /* clip-path: polygon(18.6% 0%, 0% 12.9%); */
}